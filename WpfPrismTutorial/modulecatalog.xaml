<?xml version="1.0" encoding="utf-8" ?>
<Modularity1:ModuleCatalog
    xmlns:Modularity1="clr-namespace:Prism.Modularity;assembly=Prism.Wpf"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib"
  xmlns:Modularity="clr-namespace:Microsoft.Practices.Prism.Modularity;assembly=Microsoft.Practices.Prism">
    <Modularity1:ModuleInfo
      Ref="ModuleA.dll"
      ModuleName="ModuleA"
      ModuleType="ModuleA.ModuleA, ModuleA, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
    <Modularity1:ModuleInfo
      Ref="ModuleB.dll"
      ModuleName="ModuleB"
      ModuleType="ModuleB.ModuleB, ModuleB, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
        <Modularity1:ModuleInfo.DependsOn>
            <sys:String>ModuleA</sys:String>
        </Modularity1:ModuleInfo.DependsOn>
    </Modularity1:ModuleInfo>
</Modularity1:ModuleCatalog>